<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.n52.series.api.v1.db.da.beans">
    <class name="ObservationEntity" table="observation">
        <id name="pkid" type="long">
            <column name="observationid" />
            <generator class="assigned" />
        </id>
        <property name="seriesPkid" column="seriesid"  type="long" />
        <property name="timestamp" column="phenomenontime" type="timestamp" />
        <property name="deleted" formula="'F'" type="org.hibernate.type.TrueFalseType" />
        <property name="value" column="value" type="double" />
    </class>
</hibernate-mapping>
